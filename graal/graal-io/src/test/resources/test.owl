    	<owl:NamedIndividual rdf:about="individuC" />
	  </owl:members>
    </owl:sameAs>
    -->
    
    <owl:AllDisjointClasses>
	   <owl:members rdf:parseType="Collection">
	    <owl:Class rdf:about="DisjointClassA" />
