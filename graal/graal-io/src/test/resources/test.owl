	</owl:ObjectProperty>
    
    <owl:AsymmetricProperty rdf:about="AsymmetricProperty" />
    
    <owl:SymmetricProperty rdf:about="SymmetricProperty" />
    
    <owl:TransitiveProperty rdf:about="TransitiveProperty" />
    
    <owl:IrreflexiveProperty rdf:about="IrreflexiveProperty" />
    
    <!-- Does not appear -->
    <owl:FunctionalProperty rdf:about="FunctionalProperty" />
        
    <owl:InverseFunctionalProperty rdf:about="InverseFunctionalProperty" />
    
    <owl:NamedIndividual rdf:about="individuAA" >
    	<owl:sameAs rdf:resource="individuBB" />
    </owl:NamedIndividual>
    
   <!--
    <owl:sameAs>
      <owl:members rdf:parseType="Collection">
	    <owl:NamedIndividual rdf:about="individuA" />
